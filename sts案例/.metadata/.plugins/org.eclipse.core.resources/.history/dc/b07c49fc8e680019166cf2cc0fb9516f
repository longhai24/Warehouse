package com.accp.Test.entity;

import com.accp.Test.annotation.Column;

public class Stu {
	@Column(name="id",isPrimaryKey=true,type="int",length=50)
	private int id_;
	@Column(name="name",isPrimaryKey=true,type="varchar",length=50)
	private String name_;
	@Column(name="age",isPrimaryKey=true,type="int",length=50)
	private int age_;
}
