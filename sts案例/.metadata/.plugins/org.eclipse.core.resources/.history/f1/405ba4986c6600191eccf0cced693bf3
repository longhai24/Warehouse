package com.accp.T1.entity;

import com.accp.T1.annotation.runs;

@runs(goodmoney="4000")
public class user {
	private Integer id;
	private String name;
	private String sex;
	private String add;
	public user() {
		
	}
	public user(Integer id, String name, String sex, String add) {
		this.id = id;
		this.name = name;
		this.sex = sex;
		this.add = add;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getSex() {
		return sex;
	}
	public void setSex(String sex) {
		this.sex = sex;
	}
	public String getAdd() {
		return add;
	}
	public void setAdd(String add) {
		this.add = add;
	}
	
}
