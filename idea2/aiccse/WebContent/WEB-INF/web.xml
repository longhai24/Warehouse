<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>aiccse</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>productAction</display-name>
    <servlet-name>productAction</servlet-name>
    <servlet-class>cpm.accp.ping.action.productAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>productAction</servlet-name>
    <url-pattern>/c/goods</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>AiccseFilter</display-name>
    <filter-name>AiccseFilter</filter-name>
    <filter-class>cpm.accp.ping.filter.ReqCharsetFilter</filter-class>
    <init-param>
      <param-name>charset</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>AiccseFilter</filter-name>
    <url-pattern>*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>cpm.accp.ping.listener.Aiccselistener</listener-class>
  </listener>
  <filter>
    <display-name>WhiteFilter</display-name>
    <filter-name>WhiteFilter</filter-name>
    <filter-class>cpm.accp.ping.filter.WhiteFilter</filter-class>
<!--     ç™½åå?-->
    <init-param>
      <param-name>publicPage1</param-name>
      <param-value>ui/login.html</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>WhiteFilter</filter-name>
    <url-pattern>*.html</url-pattern>
  </filter-mapping>
</web-app>