<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>firstlesson</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>StudentAction</display-name>
    <servlet-name>StudentAction</servlet-name>
    <servlet-class>com.accp.one.action.StudentAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentAction</servlet-name>
    <url-pattern>/c/student</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>TenderAction</display-name>
    <servlet-name>TenderAction</servlet-name>
    <servlet-class>com.accp.tow.action.TenderAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TenderAction</servlet-name>
    <url-pattern>/c/tender</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>StaffAction</display-name>
    <servlet-name>StaffAction</servlet-name>
    <servlet-class>com.accp.ajax.action.StaffAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StaffAction</servlet-name>
    <url-pattern>/c/staff</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SectionAction</display-name>
    <servlet-name>SectionAction</servlet-name>
    <servlet-class>com.accp.ajax2.action.SectionAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SectionAction</servlet-name>
    <url-pattern>/c/section</url-pattern>
  </servlet-mapping>
</web-app>